{%- from 'nationalarchives/components/button/macro.njk' import tnaButton -%}
{%- from 'nationalarchives/components/panel/macro.njk' import tnaPanel -%}
{% from "nationalarchives/templates/partials/logo.njk" import tnaLogo %}

<!DOCTYPE html>
<html lang="en" class="tna-template tna-template--green-accent tna-template--system-theme">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>{{ subject }}</title>
    <link rel="preconnect" href="https://use.typekit.net">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <!-- COMPILED_CSS -->
    <link rel="stylesheet" href="https://use.typekit.net/kaq6qqh.css" media="screen,print">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400..700&family=Roboto+Mono:wght@400..500&display=swap">
    <link rel="stylesheet" href="https://www.nationalarchives.gov.uk/enrichment/css/logo-adornments.css" media="screen">
  </head>
  <body class="tna-template__body">
    <div class="tna-template__main">
      <table role="presentation" border="0" cellpadding="0" cellspacing="0" width="600">
        <tr>
          <td>
            <div class="tna-email-header">
              {{ tnaLogo({size: 80, adornable: true}) }}
            </div>
          </td>
        </tr>
        <tr>
          <td>
            <div class="tna-template__content">
              {%- if panel %}
              {{ tnaPanel(panel) }}
              {%- endif %}
              {{ content | safe }}
              {%- if cta_buttons %}
              <div class="tna-button-group">
                {%- for button in cta_buttons %}
                {{ tnaButton(button) }}
                {%- endfor %}
              </div>
              {%- endif %}
              {{ signoff | safe }}
            </div>
          </td>
        </tr>
        <tr>
          <td>
            <div class="tna-email-footer">
              <table role="presentation" border="0" cellpadding="0" cellspacing="0">
                <tr>
                  <td>
                    <table role="presentation" border="0" cellpadding="0" cellspacing="0">
                      <tr>
                        <td width="80" valign="middle">
                          {{ tnaLogo({size: 80}) }}
                        </td>
                        <td width="24"></td>
                        <td valign="middle">
                          <h2 class="tna-heading-s tna-email-footer__title" translate="no">The National Archives</h2>
                          <address class="tna-email-footer__address" translate="no">
                            Kew, Richmond<br>
                            TW9 4DU
                          </address>
                          <p class="tna-email-footer__email"><a href="https://www.nationalarchives.gov.uk">nationalarchives.gov.uk</a></p>
                        </td>
                      </tr>
                    </table>
                  </td>
                </tr>
                <tr>
                  <td height="24"></td>
                </tr>
                <tr>
                  <td>
                    <hr>
                  </td>
                </tr>
                <tr>
                  <td height="24"></td>
                </tr>
                <tr>
                  <td>
                    <table role="presentation" border="0" cellpadding="0" cellspacing="0">
                      <tr>
                        <td>
                          <h3 class="tna-!--visually-hidden">
                            Follow us on social media
                          </h3>
                          <nav class="tna-email-footer__social" aria-label="Social">
                            <ul class="tna-ul tna-ul--plain tna-email-footer__social-items">
                              <li class="tna-email-footer__social-item">
                                <a href="https://twitter.com/UKNatArchives" class="tna-email-footer__social-item-link">
                                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 640" height="32" aria-hidden="true" focusable="false"><!--!Font Awesome Free v7.0.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2025 Fonticons, Inc.--><path d="M453.2 112L523.8 112L369.6 288.2L551 528L409 528L297.7 382.6L170.5 528L99.8 528L264.7 339.5L90.8 112L236.4 112L336.9 244.9L453.2 112zM428.4 485.8L467.5 485.8L215.1 152L173.1 152L428.4 485.8z"></path></svg>
                                  <span class="tna-email-footer__social-item-link-text">
                                    X (formerly Twitter)
                                  </span>
                                </a>
                              </li>
                              <li class="tna-email-footer__social-item">
                                <a href="https://www.youtube.com/c/TheNationalArchivesUK" class="tna-email-footer__social-item-link">
                                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 640" height="32" aria-hidden="true" focusable="false"><!--!Font Awesome Free v7.0.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2025 Fonticons, Inc.--><path d="M581.7 188.1C575.5 164.4 556.9 145.8 533.4 139.5C490.9 128 320.1 128 320.1 128C320.1 128 149.3 128 106.7 139.5C83.2 145.8 64.7 164.4 58.4 188.1C47 231 47 320.4 47 320.4C47 320.4 47 409.8 58.4 452.7C64.7 476.3 83.2 494.2 106.7 500.5C149.3 512 320.1 512 320.1 512C320.1 512 490.9 512 533.5 500.5C557 494.2 575.5 476.3 581.8 452.7C593.2 409.8 593.2 320.4 593.2 320.4C593.2 320.4 593.2 231 581.8 188.1zM264.2 401.6L264.2 239.2L406.9 320.4L264.2 401.6z"></path></svg>
                                  <span class="tna-email-footer__social-item-link-text">
                                    YouTube
                                  </span>
                                </a>
                              </li>
                              <li class="tna-email-footer__social-item">
                                <a href="https://www.facebook.com/TheNationalArchives" class="tna-email-footer__social-item-link">
